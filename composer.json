{
    "name": "crazymeeks/wp-rest-api-extender",
    "description": "An extender plugin for WP Rest API",
    "keywords": [
        "WordPress REST API Router",
        "WordPress Router"
    ],
    "type": "wp-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeff Claud",
            "email": "jefferson.claud@nuworks.ph"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Crazymeeks\\WP\\": "src/",
            "Mock\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "crazymeeks/wp-rest-api-extender",
                "version": "v2.0.0",
                "source": {
                    "url": "git://github.com/crazymeeks/wp-rest-api-extender.git",
                    "type": "git",
                    "reference": "v2.0.0"
                },
                "autoload": {
                    "psr-4": {
                        "Crazymeeks\\WP\\": "src/"
                    }
                }
            }
            
        }
    ]
}
